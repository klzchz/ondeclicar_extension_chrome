{
    "name": "JL- Extension",
    "version": "1.0",
    "description": "Build an Extension!",
    "browser_action": {
        "default_popup": "popup.html"
      },
      "permissions":["tabs","http://*/*", "https://*/*","https://github.com/"],
      "background": [ 
        {    
          "matches": [ "http://*/*", "https://*/*","<all_urls>"], 
            "js": [ "js/jquery.js","js/font-awesome.js","js/axios.js", "js/xapp.js"], 
            "css":["css/bootstrap.css","css/font-awesome.css"] 
        }

    ],
      
    "manifest_version": 2
    
  }