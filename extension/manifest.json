{
    "name": "OndeClicar",
    "version": "1.0",
    "description": "This is a open source project who help addicted people a watch tv series and movies with high score on Imdb without google it everytime !",
    "browser_action": {
        "default_popup": "popup.html"
      },
      "icons": {
        "16": "icon.png",
        "48": "icon.png",
       "128": "icon.png" 
      },
      "permissions":["tabs","http://*/*", "https://*/*","https://github.com/"],
      "background": [ 
        {    
          "matches": [ "http://*/*", "https://*/*","<all_urls>"], 
            "js": [ "js/jquery.js","js/font-awesome.js","js/axios.js", "js/xapp.js"], 
            "css":["css/bootstrap.css","css/font-awesome.css"] 
        }

    ],
      
    "manifest_version": 2
    
  }